GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/librtmp-iOS" ${PODS_ROOT}/librtmp-iOS/include/**
LIBRARY_SEARCH_PATHS = ${PODS_ROOT}/librtmp-iOS/lib/** "$(PODS_ROOT)/librtmp-iOS/lib" "$(PODS_ROOT)/librtmp-iOS/lib" "$(PODS_ROOT)/librtmp-iOS/lib"
OTHER_LDFLAGS = -ObjC -lcrypto -lrtmp -lssl
PODS_ROOT = ${SRCROOT}/Pods